<?xml version="1.0" encoding="UTF-8" ?>
<Application
    x:Class="AppPallet.App"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:AppPallet"
    xmlns:m="clr-namespace:UraniumUI.Material.Resources;assembly=UraniumUI.Material"
    xmlns:material="http://schemas.enisn-projects.io/dotnet/maui/uraniumui/material"
    UserAppTheme="Light">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles/DatePickerFieldStyle.xaml" />
                <ResourceDictionary Source="Resources/Styles/Colors.xaml" />
                <ResourceDictionary Source="Resources/Styles/Styles.xaml" />
                <ResourceDictionary x:Name="MisEstilos" Source="Resources/Styles/MyStyles.xaml" />
                <m:StyleResource />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
